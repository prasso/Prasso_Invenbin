{
    "name": "faxt/invenbin",
    "description": "inventory management for Laravel projects",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "filament/filament": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Faxt\\Invenbin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Faxt\\Invenbin\\InvenbinServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
